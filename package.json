{
  "name": "ant-design-playground",
  "version": "1.0.0",
  "description": "ant-design react redux playground (based on Justin Lu <gdjyluxiaoyong@gmail.com>)",
  "main": "index.js",
  "scripts": {
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "start:prod": "npm run build && npm run open:dist",
    "start:mock": "babel-node tools/mock.js",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist && npm run lint && npm run test",
    "build": "babel-node tools/build.js",
    "test": "mocha tools/testSetup.js \"src/**/*.spec.js\"",
    "test:cover": "babel-node node_modules/isparta/bin/isparta cover --root src --report html node_modules/mocha/bin/_mocha -- --require ./tools/testSetup.js \"src/**/*.spec.js\" --reporter progress",
    "test:cover:travis": "babel-node node_modules/isparta/bin/isparta cover --root src --report lcovonly _mocha -- --require ./tools/testSetup.js \"src/**/*.spec.js\" && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "npm run test -- --watch"
  },
  "private": true,
  "dependencies": {
    "antd": "1.10.0",
    "dateformat": "1.0.12",
    "es6-promise": "3.2.1",
    "isomorphic-fetch": "2.2.1",
    "js-cookie": "2.1.2",
    "object-assign": "4.1.0",
    "react": "15.3.1",
    "react-dom": "15.3.1",
    "react-redux": "4.4.5",
    "react-router": "2.7.0",
    "react-router-redux": "4.0.5",
    "redux": "3.5.2",
    "redux-promise": "0.5.3",
    "redux-thunk": "2.1.0"
  },
  "devDependencies": {
    "babel-cli": "6.14.0",
    "babel-core": "6.13.2",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.5",
    "babel-plugin-antd": "0.5.1",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.13.0",
    "babel-preset-stage-2": "6.13.0",
    "babel-register": "6.14.0",
    "browser-sync": "2.14.0",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "cheerio": "0.22.0",
    "connect-history-api-fallback": "1.3.0",
    "coveralls": "2.11.12",
    "cross-env": "2.0.0",
    "css-loader": "0.24.0",
    "enzyme": "2.4.1",
    "eslint": "3.3.1",
    "eslint-plugin-import": "1.14.0",
    "eslint-plugin-jsx-a11y": "2.2.0",
    "eslint-plugin-react": "6.1.2",
    "eslint-watch": "2.1.14",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.22.0",
    "isparta": "4.0.0",
    "json-server": "0.8.18",
    "less": "2.7.1",
    "less-loader": "2.2.3",
    "mocha": "3.0.2",
    "nock": "8.0.0",
    "node-sass": "3.8.0",
    "npm-run-all": "3.0.0",
    "ora": "0.3.0",
    "react-addons-test-utils": "15.3.1",
    "redux-immutable-state-invariant": "1.2.3",
    "redux-logger": "2.6.1",
    "redux-mock-store": "1.1.4",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.0",
    "sinon": "1.17.5",
    "sinon-chai": "2.8.0",
    "style-loader": "0.13.1",
    "webpack": "1.13.2",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.12.2"
  },
  "author": "Marius Ileana",
  "license": "ISC"
}
